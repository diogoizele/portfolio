{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "CommonJS",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "baseUrl": "./src",
    "paths": {
      "components/*": ["components/*"],
      "assets/*": ["assets/*"],
      "constants/*": ["constants/*"],
      "containers/*": ["containers/*"],
      "models/*": ["models/*"],
      "pages/*": ["pages/*"],
      "styles/*": ["styles/*"],
      "utils/*": ["utils/*"],
      "static/*": ["static/*"]
    },
    "moduleResolution": "node"
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "next-sitemap.config.js"],
  "exclude": ["node_modules"]
}
